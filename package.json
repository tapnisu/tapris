{
  "name": "tapris",
  "description": "Multipurpose discord bot",
  "version": "3.4.3",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "start:watch": "node . --watch",
    "start:migrate:prod": "prisma migrate deploy && node .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prod": "tsc && node .",
    "test": "node --test",
    "commands:export": "node ./dist/commandsExport.js",
    "commands:delete": "node ./dist/commandsDelete.js",
    "format": "prettier . --check --ignore-path .gitignore",
    "format:fix": "prettier . --write --ignore-path .gitignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": {
    "email": "aleksei@tapni.su",
    "name": "Aleksei Rybin",
    "url": "https://tapni.su/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapnisu/tapris.git"
  },
  "bugs": {
    "url": "https://github.com/tapnisu/tapris/issues"
  },
  "keywords": [
    "bot",
    "discord"
  ],
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.18.0",
    "@iamtraction/google-translate": "^2.0.1",
    "@napi-rs/canvas": "^0.1.67",
    "@prisma/client": "^5.22.0",
    "avconv": "^3.1.0",
    "axios": "^1.7.9",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "ffmpeg": "^0.0.4",
    "genshin-db": "^5.1.12",
    "html-to-text": "^9.0.5",
    "i18n": "^0.15.1",
    "sodium-native": "^4.3.1",
    "typeface-jetbrains-mono": "^2.0.1",
    "waifu.js": "^1.0.0",
    "youtube-sr": "^4.3.11",
    "ytdl-core": "^4.11.5"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "erlpack": "^0.1.4",
    "utf-8-validate": "^6.0.5",
    "zlib-sync": "^0.1.9"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/js": "^9.20.0",
    "@types/i18n": "^0.13.12",
    "@types/node": "^18.19.75",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "node-gyp": "^11.1.0",
    "prettier": "^3.5.0",
    "prisma": "^5.22.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "engines": {
    "node": ">=18"
  },
  "imports": {
    "#commands/*": "./dist/commands/*",
    "#components/*": "./dist/components/*",
    "#core/*": "./dist/core/*",
    "#events/*": "./dist/events/*",
    "#interfaces/*": "./dist/interfaces/*",
    "#lib/*": "./dist/lib/*",
    "#locales/*": "./dist/locales/*",
    "#db/*": "./dist/db/*"
  },
  "type": "module",
  "packageManager": "pnpm@9.15.5+sha512.845196026aab1cc3f098a0474b64dfbab2afe7a1b4e91dd86895d8e4aa32a7a6d03049e2d0ad770bbe4de023a7122fb68c1a1d6e0d033c7076085f9d5d4800d4"
}
